        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-latest.xsd"
        logicalFilePath="../db/changelog/data/enumTables/genres.xml">

    <changeSet id="genre_alternative_metal" author="${author}">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="0">
                SELECT COUNT(1) FROM genre AS g WHERE g.name = "Alternative Metal"
            </sqlCheck>
        </preConditions>
        <insert tableName="genre">
            <column name="name" value="Alternative Metal"/>
        </insert>
        <rollback>
            <sql>
                DELETE FROM genre AS g WHERE g.name = "Alternative Metal"
            </sql>
        </rollback>
    </changeSet>

